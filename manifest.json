{
  "manifest_version": 3,
  "name": "Gemini AI Assistant",
  "version": "1.0.0",
  "description": "Your AI companion for Chat, Search, Maps, and Translation powered by Gemini.",
  "permissions": [
    "geolocation"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Gemini AI Assistant"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}